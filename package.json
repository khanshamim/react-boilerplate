{
  "name": "react-boilerplate-ui",
  "version": "1.0.0",
  "description": "React Boiler plate UI",
  "author": "",
  "private": true,
  "main": "index.js",
  "scripts": {
    "install:io": "yarn install --ignore-optional  --network-timeout 600000",
    "clean": "rimraf dist/*",
    "start": "webpack-dev-server --config webpack.config.local.js",
    "build:wp": "webpack --config webpack.config.prod.js --bail --cache",
    "build": "yarn run clean && yarn run build:wp",
    "build:ci": "yarn run build",
    "test:without-coverage": "yarn run jest --logHeapUsage --maxWorkers=2 --config jest.config.js",
    "test:with-coverage": "yarn run test:without-coverage --coverage",
    "test:once": "jest",
    "test": "yarn run test:with-coverage",
    "test:watch": "yarn run test:without-coverage --watch",
    "prettier:check": "prettier --ignore-unknown --check .",
    "prettier:fix": "prettier --ignore-unknown --write ."
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.9.1",
    "@mui/styles": "^5.9.1",
    "@reduxjs/toolkit": "^1.8.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.3.5",
    "react-redux": "^8.0.2",
    "react-router": "^6.8.0",
    "react-router-dom": "^6.8.0",
    "react-swipeable-views": "^0.14.0"
  },
  "jestSonar": {
    "reportPath": "target/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-decorators": "^7.17.8",
    "@babel/plugin-proposal-function-bind": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.4",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "jest-junit": "^13.0.0",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.6.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  }
}
